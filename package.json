{
	"name": "app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "react-native start",
		"s": "react-native start -- --reset-cache",
		"test": "jest",
		"link": "react-native link",
		"lint": "eslint .",
		"prettify": "prettier --write 'src/**/*.js'",
		"android": "react-native run-android",
		"a": "npm run android",
		"android:production": "react-native run-android --variant=release",
		"a:p": "npm run android:production",
		"android:install": "adb install -r android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk",
		"a:i": "npm run android:install",
		"android:run": "npm run a:a && npm run a:i",
		"a:r": "npm run android:run",
		"android:clean": "cd android && ./gradlew clean && cd ..",
		"a:c": "npm run android:clean",
		"android:assemble": "rm -rf android/app/build/outputs/apk/release/*.apk && cd android && ./gradlew assembleRelease && cd ..",
		"a:a": "npm run android:assemble",
		"android:bundle": "cd android &&./gradlew bundleRelease && cd ..",
		"a:b": "npm run android:bundle",
		"ios": "react-native run-ios --simulator='iPhone 5s'",
		"i": "npm run ios",
		"ios:simulator": "react-native run-ios",
		"i:s": "npm run ios:simulator",
		"ios:device": "react-native run-ios --device",
		"i:d": "npm run ios:device",
		"ios:production": "react-native run-ios --configuration Release",
		"i:p": "npm run ios:production",
		"ios:device:production": "react-native run-ios --configuration Release --device",
		"i:d:p": "npm run ios:device:production",
		"ios:pod:install": "cd ios && pod install && cd ..",
		"i:p:i": "npm run ios:pod:install",
		"ios:pod:update": "cd ios && pod update && cd ..",
		"i:p:u": "npm run ios:pod:install",
		"backend": "node backendTestServer.js",
		"manage:native:files": "node docs/nativeFilesManager.js",
		"m:n:f": "node docs/nativeFilesManager.js",
		"cache:clear": "rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && watchman watch-del-all && npm cache verify",
		"c:c": "npm run cache:clear",
		"rebuild": "npm run c:c && rm -rf node_modules package-lock.json && npm i && react-native link && cd ios && pod deintegrate && pod install && cd ..",
		"r": "npm run rebuild"
	},
	"dependencies": {
		"@react-native-community/async-storage": "^1.6.2",
		"@react-native-community/netinfo": "^4.4.0",
		"@react-native-community/push-notification-ios": "^1.0.3",
		"axios": "^0.19.0",
		"collect.js": "^4.18.9",
		"http-status-codes": "^1.4.0",
		"i18next": "^17.2.0",
		"moment": "^2.24.0",
		"native-base": "^2.13.8",
		"react": "16.9.0",
		"react-i18next": "^10.13.1",
		"react-native": "0.61.4",
		"react-native-animatable": "^1.3.3",
		"react-native-background-fetch": "^2.7.1",
		"react-native-calendars": "^1.217.0",
		"react-native-device-info": "^5.4.0",
		"react-native-geolocation-service": "^3.1.0",
		"react-native-gesture-handler": "^1.4.1",
		"react-native-keyboard-aware-scroll-view": "^0.9.1",
		"react-native-maps": "0.26.1",
		"react-native-modal": "^11.5.3",
		"react-native-modal-datetime-picker": "^7.6.0",
		"react-native-permissions": "^2.0.2",
		"react-native-push-notification": "^3.1.9",
		"react-native-reanimated": "^1.2.0",
		"react-native-screens": "^1.0.0-alpha.23",
		"react-native-search-bar": "^3.5.1",
		"react-native-swipe-list-view": "^2.0.3",
		"react-navigation": "^4.0.8",
		"react-navigation-drawer": "^2.2.2",
		"react-navigation-stack": "^1.9.0",
		"react-redux": "^7.1.1",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8",
		"redux-logger": "^3.0.6",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.1",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/core": "7.6.2",
		"@babel/runtime": "7.6.2",
		"@types/jest": "^24.0.23",
		"@types/node": "^12.12.14",
		"@types/react": "^16.9.13",
		"@types/react-native": "^0.60.23",
		"@types/react-native-push-notification": "^3.0.7",
		"@types/react-redux": "^7.1.5",
		"@types/react-test-renderer": "^16.9.1",
		"@types/uuid": "^3.4.6",
		"babel-eslint": "^10.0.3",
		"babel-jest": "24.9.0",
		"dotenv": "^8.2.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.3.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react-hooks": "^2.3.0",
		"express": "^4.17.1",
		"ip": "^1.1.5",
		"jest": "24.9.0",
		"metro-react-native-babel-preset": "0.54.1",
		"react-native-json-tree": "^1.2.0",
		"react-test-renderer": "16.9.0",
		"xml2js": "^0.4.22"
	},
	"jest": {
		"preset": "react-native"
	}
}
